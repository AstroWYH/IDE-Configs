        // 1. 强制开启全局语义高亮
        ,"editor.semanticHighlighting.enabled": true,

        // 2. (关键) 告诉 C# 插件“我要增强型高亮”
        // 如果你是用新版 C# Dev Kit，这个通常默认开启，但旧版或者某些环境下需要显式设置
        "csharp.semanticHighlighting.enabled": true, 

        // =========================================================
        // 1. 语义高亮 (Semantic) - 优先级最高，负责精准识别
        // =========================================================
            // =========================================================
            // 1. 语义高亮 (Semantic) - 这里的配置会覆盖下面的正则配置
            //    核心：依靠这里把“使用处”的 Field 颜色救回来
            // =========================================================
            "editor.semanticTokenColorCustomizations": {
                "enabled": true,
                "rules": {
                    // --- 【类、接口、结构体】 ---
                    "class": "#4EC9B0",
                    "struct": "#4EC9B0",
                    "interface": "#6B8E23",
                    "enum": "#4EC9B0",
                    "delegate": "#4EC9B0",
                    "namespace": "#E5C07B", 
        
                    // --- 【函数 / 方法】 ---
                    "method": "#eb6f8c",
        
                    // --- 【属性 Property】 ---
                    "property": "#97A054", 
                    
                    // --- 【成员变量 Field】(修正重点) ---
                    "field": "#6E9297",          // 普通成员变量
                    "field.readonly": "#6E9297", // ★ 新增：只读字段 (不加这个，readonly字段可能会变色)
                    "field.static": "#6E9297",   // ★ 新增：静态字段 (不加这个，static字段可能会变色)
                    "enumMember": "#a26a42",
        
                    // --- 【局部变量 & 参数】 ---
                    "variable": "#8490D5",       // 局部变量
                    "parameter": "#FF8C00",      // 参数
        
                    // --- 【其他基础】 ---
                    "keyword": "#b69ae1",
                    "string": "#CE9178",
                    "number": "#9cb68d",
                    "comment": "#B9B0B9"
                }
            },
        
            // =========================================================
            // 2. TextMate 规则 - 这里的配置负责“兜底”和“定义处”
            // =========================================================
            "editor.tokenColorCustomizations": {
                "textMateRules": [
                    // --- 【类】 ---
                    {
                        "scope": ["entity.name.type", "storage.type"],
                        "settings": { "foreground": "#50C09C" }
                    },
                    // --- 【接口】 ---
                    {
                        "scope": "entity.name.type.interface",
                        "settings": { "foreground": "#eb6f8c" }
                    },
                    // --- 【函数】 ---
                    {
                        "scope": ["entity.name.function", "support.function"],
                        "settings": { "foreground": "#eb6f8c" }
                    },
                    // --- 【属性 Property】 ---
                    {
                        "scope": ["entity.name.variable.property", "variable.other.object.property"],
                        "settings": { "foreground": "#97A054" }
                    },
                    
                    // --- 【成员变量 Field】(TextMate层) ---
                    // 注意：这一条只能控制“定义处”和“this.field”这种写法
                    {
                        "scope": ["entity.name.variable.field", "variable.other.object.field"],
                        "settings": { "foreground": "#6E9297" }
                    },
        
                    // --- 【消灭紫色 - 兜底规则】 ---
                    // 这一条非常霸道，它把所有TextMate认不出来的变量都变成了局部变量颜色(#8490D5)
                    // Field在使用时(不加this)会被这条命中，所以必须依靠上面的 Semantic Rules 来纠正
                    {
                        "scope": [
                            "variable",
                            "entity.name.variable.local",
                            "variable.other.readwrite",
                            "variable.other.object",
                            "variable.parameter"
                        ],
                        "settings": { "foreground": "#8490D5" }
                    },
        
                    // --- 【其他】 ---
                    {
                        "scope": ["keyword", "storage.modifier", "storage.type.cs"],
                        "settings": { "foreground": "#569CD6" }
                    },
                    {
                        "scope": "string",
                        "settings": { "foreground": "#CE9178" }
                    },
                    {
                        "scope": "comment",
                        "settings": { "foreground": "#6A9955" }
                    }
                ]
            },
            "workbench.list.horizontalScrolling": true
    , 
}